{
  "name": "easygeneratortest",
  "version": "1.0.0",
  "description": "Easy Generator Test Project",
  "main": "index.js",
  "scripts": {
    "cy:run": "env-cmd -f ./.env cypress run",
    "cy:run:chrome": "env-cmd -f ./.env cypress run --browser chrome",
    "cy:open": "env-cmd -f ./.env cypress open",
    "format": "npx prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Lathika Herath",
  "license": "ISC",
  "dependencies": {
    "@types/mocha": "^10.0.1",
    "env-cmd": "^10.1.0",
    "mochawesome": "^7.1.3",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "cypress": "^12.9.0",
    "prettier": "^2.8.7"
  }
}
